{
    "id": "harmonizome",
    "instances": 1,
    "cpus": 1,
    "mem": 2048,
    "container": {
        "type": "DOCKER",
        "docker": {
            "image": "maayanlab/harmonizome:1.0",
            "forcePullImage": true,
            "network": "BRIDGE",
            "portMappings": [
                {
                    "containerPort": 8080,
                    "hostPort": 0,
                    "protocol": "tcp"
                }
            ]
        }
    }
}

