{
    "id": "harmonizome",
    "instances": 1,
    "cpus": 1,
    "mem": 2048,
    "container": {
        "type": "DOCKER",
        "docker": {
            "image": "146.203.54.165:5000/harmonizome:latest",
            "forcePullImage": true,
            "network": "BRIDGE",
            "portMappings": [
                {
                    "containerPort": 8080,
                    "hostPort": 0,
                    "protocol": "tcp"
                }
            ]
        }
    },
	"healthChecks": [
    	{
            "path": "/harmonizome",
            "protocol": "HTTP",
          	"portIndex": 0,
          	"gracePeriodSeconds": 10,
          	"intervalSeconds": 60,
          	"timeoutSeconds": 10,
          	"maxConsecutiveFailures": 3
        }
    ]
}

